v0x1a1c150_0 .net "clk", 0 0, v0x1a2c260_0; 1 drivers
v0x1a2c120_0 .var "out", 7 0;
v0x1a2c1c0_0 .net "reset", 0 0, v0x1a2c330_0; 1 drivers
E_0x19f83c0 .event edge, v0x1a2c1c0_0;
E_0x19f7eb0 .event posedge, v0x1a1c150_0;
    .scope S_0x1a1c060;
T_0 ;
    %wait E_0x19f7eb0;
    %load/v 8, v0x1a2c120_0, 8;
    %mov 16, 0, 24;
   %addi 8, 1, 32;
    %ix/load 0, 8, 0;
    %assign/v0 v0x1a2c120_0, 0, 8;
    %jmp T_0;
    .thread T_0;
    .scope S_0x1a1c060;
T_1 ;
    %wait E_0x19f83c0;
    %load/v 8, v0x1a2c1c0_0, 1;
    %jmp/0xz  T_1.0, 8;
    %cassign/v v0x1a2c120_0, 0, 8;
    %jmp T_1.1;
T_1.0 ;
    %deassign v0x1a2c120_0, 0, 8;
T_1.1 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x19f87d0;
T_2 ;
    %set/v v0x1a2c330_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x19f87d0;
T_3 ;
    %delay 17, 0;
    %set/v v0x1a2c330_0, 1, 1;
    %delay 11, 0;
    %set/v v0x1a2c330_0, 0, 1;
    %delay 29, 0;
    %set/v v0x1a2c330_0, 1, 1;
    %delay 11, 0;
    %set/v v0x1a2c330_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 10 "$stop";
    %end;
    .thread T_3;
    .scope S_0x19f87d0;
T_4 ;
    %set/v v0x1a2c260_0, 0, 1;
    %end;
    .thread T_4;
    .scope S_0x19f87d0;
T_5 ;
    %delay 5, 0;
    %load/v 8, v0x1a2c260_0, 1;
    %inv 8, 1;
    %set/v v0x1a2c260_0, 8, 1;
    %jmp T_5;
    .thread T_5;
    .scope S_0x19f87d0;
T_6 ;
    %vpi_call 2 21 "$monitor", "At time %t, value = %h (%0d)", $time, v0x1a2c3e0_0, v0x1a2c3e0_0;
